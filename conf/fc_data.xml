<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dataConf xmlns="http://fc.msf/common/config/type/data">
    <!-- Initial setting config -->
    <swClustersData>
        <swClusterData>
            <swCluster>
                <!-- ID of SW cluster -->
                <swClusterId>1</swClusterId>
                <!-- Maximum number of Leaves in SW cluster -->
                <maxLeafNum>45</maxLeafNum>
                <!-- Maximum number of Spines in SW cluster -->
                <maxSpineNum>4</maxSpineNum>
                <!-- Maximum number of RRs in SW cluster -->
                <maxRrNum>2</maxRrNum>
                <!-- Modification of default value is prohibited -->
                <spineStartPos>1</spineStartPos>
                <!-- Modification of default value is prohibited -->
                <leafStartPos>51</leafStartPos>
                <!-- Modification of default value is prohibited -->
                <rrStartPos>101</rrStartPos>
                <!-- Modification of default value is prohibited -->
                <fcStartPos>201</fcStartPos>
                <!-- Modification of default value is prohibited -->
                <ecStartPos>211</ecStartPos>
                <!-- Modification of default value is prohibited -->
                <emStartPos>221</emStartPos>
                <!-- AS number of SW cluster -->
                <asNum>65000</asNum>
                <!-- OSPF area number of SW cluster -->
                <ospfArea>10</ospfArea>
                <!-- Inchannel starting IP address of SW cluster -->
                <inchannelStartAddress>1.1.1.1</inchannelStartAddress>
                <!-- Outchannel starting IP address of SW cluster -->
                <outchannelStartAddress>1.1.1.1</outchannelStartAddress>
                <!-- Aggregation starting IP address -->
                <aggregationStartAddress>1.1.1.1</aggregationStartAddress>
                <!-- Aggregation prefix -->
                <aggregationAddressPrefix>24</aggregationAddressPrefix>
                <!-- Normal IGP Cost of Internal link -->
                <internalLinkNormalIgpCost>100</internalLinkNormalIgpCost>
            </swCluster>
            <rrs>
                <rr>
                    <!-- RR node ID of SW cluster -->
                    <rrNodeId>1</rrNodeId>
                    <!-- RR router ID of SW cluster -->
                    <rrRouterId>1.1.1.1</rrRouterId>
                </rr>
                <leafRr>
                    <!-- RR belonging cluster ID of Leaf's peer -->
                    <leafRrSwClusterId>1</leafRrSwClusterId>
                    <!-- RR node ID of Leaf's peer -->
                    <leafRrNodeId>1</leafRrNodeId>
                    <!-- RR router ID of Leaf's peer -->
                    <leafRrRouterId>1.1.1.1</leafRrRouterId>
                </leafRr>
            </rrs>
        </swClusterData>
    </swClustersData>
</dataConf>

